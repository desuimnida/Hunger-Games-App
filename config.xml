<?xml version="1.0" encoding="UTF-8" ?>
    <!-- For ID, just put your name in between the com and example -->
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.LilyBalingit.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->
    
    <!-- Name of your app -->
    <name>Hunger Games</name>

    <!-- Description of your App -->
    <description>
        Hunger Games App
    </description>

    <!-- Author of the app ie, you) -->
    <author href="https://build.phonegap.com" email="lizettebalingit@yahoo.com">
        Lily Balingit
    </author>

    <!-- Your plugin code -->
    <gap:plugin name="nl.x-services.plugins.socialsharing" /> <!-- Social Sharing -->

</widget>